-
  var header='#what-i-can-do';
  var whatCanIDoList=[
    {
        title:'FrontEnd',
        skillList:[
                    'HTML5',
                    'CSS3',
                    'JavaScript',
                    'jQuery',
                    'React',
                    'JSON',
                    'UI',
                    'UX',
                    'SPA',
                    'SEO',
                    'SMACSS/OOCSS',
                    'SASS',
                    'Bootstrap',
                    'NodeJS',
                    'JADE',
                    'Pug',
                    'Gulp',
                    'PhotoShop',
                    'Illustrator',
                    'Lightroom'

        ]
    },
    {
        title:'BackEnd',
        skillList:[
                'Git',
                'CLI',
                'Nginx',
                'REST',

        ]
    },
    ];

#what-can-i-do
.what-can-i-do
  .what-can-i-do__header=header
  each whatCanIDo in whatCanIDoList
     .what-can-i-do__title=whatCanIDo.title
     .what-can-i-do__list
        each skill in whatCanIDo.skillList
            .what-can-i-do__skill=skill

